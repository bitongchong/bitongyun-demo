<%@ page language="java" contentType="text/html; charset=utf-8" 
pageEncoding="utf-8"%>
<html lang="en" >
<head>
<title>邂逅笔筒云</title>

<link rel="stylesheet" href="/static/css/style-login-success.css">
</head>

<body>
<div class="cont">
  <div class="mouse"></div>
  <div class="app">
    <div class="app__bgimg">
      <div class="app__bgimg-image app__bgimg-image--1"> </div>
      <div class="app__bgimg-image app__bgimg-image--2"> </div>
    </div>
    <div class="app__img"> <img onmousedown="return false" src="/static/img/whiteTest4.png" alt="city" /> </div>
    <div class="app__text app__text--1">
      <div class="app__text-line app__text-line--4"><a href="to_login">登 录</a> 笔 筒 云 </div>
      <div class="app__text-line app__text-line--3">遇 见</div>
      <div class="app__text-line app__text-line--2">更好 使用体验 </div>
      <div class="app__text-line app__text-line--1"><img src="/static/img/opus-attachment.png" alt="" /></div>
    </div>
    <div class="app__text app__text--2">
      <div class="app__text-line app__text-line--4"><a href="to_register">注 册</a> 笔 筒 云</div>
      <div class="app__text-line app__text-line--3">拥 有</div>
      <div class="app__text-line app__text-line--2">全新 存储方式</div>
      <div class="app__text-line app__text-line--1"><img src="/static/img/opus-attachment.png" alt="" /></div>
    </div>
  </div>
  <div class="pages">
    <ul class='pages__list'>
      <li data-target='1' class='pages__item pages__item--1 page__item-active'></li>
      <li data-target='2' class='pages__item pages__item--2'></li>
    </ul>
  </div>
</div>

<script src="/static/js/jquery-1.10.2.js"></script> 
<script  src="/static/js/index.js"></script>
</body>
</html>
